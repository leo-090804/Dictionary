<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="376.0" prefWidth="615.0" style="-fx-background-color: #ADD8E6;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="DicFX.Modify.EditWord.EditWord">
    <children>
        <TextField id="newWord" fx:id="newWord" alignment="TOP_LEFT" layoutX="40.0" layoutY="28.0" prefHeight="32.0" prefWidth="399.0" promptText="New word">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font></TextField>
        <TextField id="newWordType" fx:id="newWordType" layoutX="40.0" layoutY="85.0" prefHeight="6.0" prefWidth="525.0" promptText="New word type">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font></TextField>
        <TextField id="newMeaning" fx:id="newMeaning" alignment="TOP_LEFT" layoutX="40.0" layoutY="139.0" prefHeight="214.0" prefWidth="525.0" promptText="New meaning">
         <font>
            <Font name="Times New Roman" size="24.0" />
         </font></TextField>
        <Button id="confirmButton" fx:id="confirmButton" layoutX="565.0" layoutY="23.0" mnemonicParsing="false" onMouseClicked="#editWord" prefHeight="41.0" prefWidth="42.0" text="Confirm" textAlignment="JUSTIFY">
         <font>
            <Font name="Times New Roman Bold" size="18.0" />
         </font></Button>
      <Button layoutX="509.0" layoutY="23.0" mnemonicParsing="false" onMouseClicked="#deleteWord" prefHeight="41.0" prefWidth="42.0" text="Delete" textAlignment="CENTER">
         <font>
            <Font name="Times New Roman Bold" size="18.0" />
         </font></Button>
      <Button layoutX="454.0" layoutY="23.0" mnemonicParsing="false" onMouseClicked="#setConfirmButton" prefHeight="41.0" prefWidth="42.0" text="Edit">
         <font>
            <Font name="Times New Roman Bold" size="18.0" />
         </font></Button>
    </children>
</AnchorPane>
